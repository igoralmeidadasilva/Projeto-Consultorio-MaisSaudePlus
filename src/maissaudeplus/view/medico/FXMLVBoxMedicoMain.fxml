            <Menu mnemonicParsing="false" text="Trocar Usúario">
               <items>
                  <MenuItem fx:id="menuItemTrocarFuncionario" mnemonicParsing="false" onAction="#handleMenuItemTrocarFuncionario" text="Funcionário" />
