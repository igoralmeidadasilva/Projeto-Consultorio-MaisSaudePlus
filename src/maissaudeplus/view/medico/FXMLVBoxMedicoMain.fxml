                        <Font name="System Bold" size="12.0" />
                     </font>
                     <padding>
                        <Insets left="22.0" />
                     </padding>
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" opacity="0.4" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../icons/question_mark_FILL0_wght400_GRAD0_opsz48.png" />
                           </image>
                           <cursor>
                              <Cursor fx:constant="DEFAULT" />
                           </cursor>
                           <effect>
                              <Blend mode="DARKEN" />
                           </effect>
                        </ImageView>
                     </graphic>
                  </JFXButton>
